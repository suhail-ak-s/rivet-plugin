{
  "name": "rivet-intermediate-output",
  "packageManager": "yarn@3.5.0",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/bundle.js",
  "description": "A Rivet plugin for capturing and displaying intermediate outputs for debugging and analysis",
  "author": {
    "name": "Suhail Ak",
    "url": "https://github.com/suhail"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/suhail/rivet-intermediate-output"
  },
  "private": true,
  "workspaces": [
    "."
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && tsx bundle.ts",
    "dev": "run-p watch:tsc watch:esbuild:sync",
    "watch:tsc": "tsc -b -w --preserveWatchOutput",
    "watch:esbuild": "tsx bundle.ts --watch",
    "watch:esbuild:sync": "tsx bundle.ts --watch --sync",
    "publish": "yarn npm publish --access public",
    "test": "jest"
  },
  "dependencies": {
    "@ironclad/rivet-core": "^1.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.0",
    "esbuild": "^0.19.2",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "recursive-copy": "^2.0.14",
    "ts-jest": "^29.1.0",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "20.6.1"
  },
  "rivet": {
    "skipInstall": true
  }
}
